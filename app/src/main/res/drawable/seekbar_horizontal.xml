<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
        <!-- 背景图 -->
        <item android:id="@+id/background" android:drawable="@drawable/seekbar_bkg_red" />
        <!-- 第二进度图 -->
        <item android:id="@+id/SecondaryProgress" android:drawable="@drawable/seekbar_bkg_red" >
            <clip>
                <shape>
                    <corners android:radius="5dip" />
                </shape>
            </clip>
        </item>
        <!-- 进度度 -->
        <item android:id="@+id/progress" android:drawable="@drawable/seekbar_bkg" />
    </layer-list>
</layer-list>